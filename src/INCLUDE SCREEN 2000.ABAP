MODULE status_2000 OUTPUT.

  SET PF-STATUS 'PF_RPS'.
  SET TITLEBAR 'PF_TIT'.

ENDMODULE.
*&---------------------------------------------------------------------*
*&      Module  USER_COMMAND_2000  INPUT
*&---------------------------------------------------------------------*
*       text
*----------------------------------------------------------------------*
MODULE user_command_2000 INPUT.

  CASE sy-ucomm.
    WHEN 'BACK'.
      LEAVE TO SCREEN 0.
    WHEN 'EXIT'.
      LEAVE TO SCREEN 0.
    WHEN 'CANCEL'.
      LEAVE PROGRAM.
    WHEN '&AGAIN'.
      LEAVE TO SCREEN 0.
  ENDCASE.

ENDMODULE.
